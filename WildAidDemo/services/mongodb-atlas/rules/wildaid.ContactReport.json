{
    "database": "wildaid",
    "collection": "ContactReport",
    "roles": [
        {
            "name": "Global Admin",
            "apply_when": {
                "%%true": {
                    "%function": {
                        "arguments": [
                            "%%user.data.email"
                        ],
                        "name": "isGlobalAdmin"
                    }
                }
            },
            "write": true,
            "insert": true,
            "delete": true,
            "additional_fields": {}
        },
        {
            "name": "Agency Admin",
            "apply_when": {
                "%%true": {
                    "%function": {
                        "arguments": [
                            "%%user.data.email"
                        ],
                        "name": "isAgencyAdmin"
                    }
                }
            },
            "write": true,
            "insert": true,
            "delete": true,
            "additional_fields": {}
        },
        {
            "name": "Agency Member",
            "apply_when": {
                "%%true": {
                    "%function": {
                        "arguments": [
                            "%%root.agency",
                            "%%user.data.email"
                        ],
                        "name": "isAgencyMember"
                    }
                }
            },
            "write": true,
            "insert": true,
            "delete": false,
            "additional_fields": {}
        }
    ],
    "schema": {}
}
